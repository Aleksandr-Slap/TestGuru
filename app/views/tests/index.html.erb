<div class="card justify-content-center mt-3 border-primary">
  <div class="card-body">    
    <h1 class="text-center"><%= t('.header') %></h1>

    <table class="table table-bordered table-hover table-success table-striped text-center">
      <tr>
        <th class="sort-by-title pointer">
          <%= t('.title') %>
          <%= octicon 'arrow-up', class: 'text-success hide' %>
          <%= octicon 'arrow-down', class: 'text-success hide' %>
        </th>
        <th><%= t('.level') %></th>
        <th><%= t('.questions') %></th>
        <th><%= t('.actions') %></th>
      </tr>
      <%= render @tests %>
      
    </table> 
  </div>
</div>
  
  