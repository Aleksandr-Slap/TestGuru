<h1><%= test_header(@test) %></h1>
<%= form_with model: @test do |form| %>

  <%= render "shared/errors", resource: @test %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %><br>
  </p>
  
  <p>  
    <%= form.label :level %><br>
    <%= form.number_field :level %><br>
  </p>
  
  <p>  
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>
  
  <p>  
    <%= form.label :user_id %>
    <%= form.collection_select :user_id, User.all, :id, :name, prompt: true %>
  </p>
  
  <p><%= form.submit %></p> 
<% end %>