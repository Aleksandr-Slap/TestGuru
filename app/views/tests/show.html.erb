<h1>Test questions</h1>


<ul>  
  <%@test.questions.each do |question|%>
    <li>
      <%= question.body%>
    </li>  
  <% end %>    
</ul>

<h2>Add a question:</h2>
<%= form_with model: [ @test, @test.questions.build ] do |form| %>
  <p>
    <%= form.label :body %><br>
    <%= form.text_area :body %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>

