<%= form_with model: [:admin, @test] do |form| %>

  <%= render "shared/errors", resource: @test %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %><br>
  </p>
  
  <p>  
    <%= form.label :level %><br>
    <%= form.number_field :level %><br>
  </p>
  
  <p>  
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>

  <p>
    <%= form.label :timer %><br>
    <%= form.number_field :timer %>
  </p>

  <p>
    <%= form.label :ready %><br>
    <%= form.check_box :ready %>
  </p>
  
  <p><%= form.submit %></p> 
<% end %>